<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="vr_phone">



<operation name="selectPhone" resultClass="java.util.HashMap" multiplicity="many">
	<sql>
		select * from vr_phone where 1 = 1
	</sql>
	<sqlmap>
		<![CDATA[
		SELECT PHONE_TYPE  name ,COUNT(1)  value FROM vr_phone  GROUP BY PHONE_TYPE
		]]>
	</sqlmap>
</operation>
<operation name="selectOs" resultClass="java.util.HashMap" multiplicity="many">
	<sql>
		select * from vr_phone where 1 = 1
	</sql>
	<sqlmap>
		<![CDATA[
		SELECT SYS_VERSION  name ,COUNT(1)  value FROM vr_phone  GROUP BY SYS_VERSION
		]]>
	</sqlmap>
</operation>
<operation name="selectAddress" resultClass="java.util.HashMap" multiplicity="many">
	<sql>
		select * from vr_phone where 1 = 1
	</sql>
	<sqlmap>
		<![CDATA[
		SELECT PROVINCE name ,COUNT(1)  value FROM vr_phone  GROUP BY PROVINCE
		]]>
	</sqlmap>
</operation>
</table>