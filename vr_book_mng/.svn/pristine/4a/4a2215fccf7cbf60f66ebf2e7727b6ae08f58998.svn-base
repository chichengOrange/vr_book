    <!-- mapped statement for IbatisGiftRecordDAO.selectallgiftrecord -->
	<select id="MS-GIFT-RECORD-SELECTALLGIFTRECORD" resultClass="java.util.HashMap">
	        select dgr.*,dg.GIFT_NAME,dg.GIFT_URL,dg.GIFT_COST,dg.GIFT_TYPE,du.NICK_NAME,edu.NICK_NAME ENICK_NAME 
				 from dxs_gift_record dgr 
				left join dxs_gift dg on dgr.GIFT_ID = dg.GIFT_ID 
				left join dxs_user du on dgr.USER_ID = du.USER_ID
				left join dxs_user edu on dgr.EXCEPTIONAL_USER_ID = edu.USER_ID
				where 1=1 
				<isNotEmpty property="nickname">
	    			AND du.NICK_NAME like '%$nickname$%'
	    		</isNotEmpty>
	    		<isNotEmpty property="enickname">
	    			AND edu.NICK_NAME like '%$enickname$%'
	    		</isNotEmpty>
	    		<isNotEmpty property="name">
	    			AND dg.GIFT_NAME like '%$name$%'
	    		</isNotEmpty>
	    		<isNotEmpty property="type">
	    			AND dg.GIFT_TYPE = #type#
	    		</isNotEmpty>
	    		<isNotEmpty property="starpdate">
	    			AND dgr.EXCEPTIONAL_TIME &gt;= #starpdate#
	    		</isNotEmpty>
	    		<isNotEmpty property="enddate">
	    			AND dgr.EXCEPTIONAL_TIME &lt;= #enddate#
	    		</isNotEmpty>
	    		order by dgr.EXCEPTIONAL_TIME desc 
	    		LIMIT $offset$, $limit$
    </select>

