    <!-- mapped statement for IbatisCallOrderDAO.exportorder -->
	<select id="MS-CALL-ORDER-EXPORTORDER" resultClass="java.util.HashMap">
	        SELECT @rownum:=@rownum+1 AS ROWNUM, d.*
				FROM (SELECT @rownum:=0) r,
				(
					SELECT
						allorder.*, CASE
					WHEN allorder.ORDER_STATUS = '1' THEN
						'1'
					WHEN allorder.ORDER_STATUS = '0' THEN
						'0'
					WHEN allorder.ORDER_STATUS = '2' THEN
						'2'
					WHEN allorder.ORDER_STATUS = '3' THEN
						'3'
					ELSE
						''
					END ORDERSTATUS
					FROM
						(
							SELECT
								dco.ORDER_NO,
								dco.COST,
								dco.ORDER_STATUS,
								dco.GMT_CREATE,
								'1' TYPE,
								du.USER_NAME,
								du.NICK_NAME,
								du.USER_ID
							FROM
								dxs_call_order dco
							LEFT JOIN dxs_user du ON dco.CALLED_USER_ID = du.USER_ID
							UNION
								SELECT
									dmo.ORDER_NO,
									dmo.PAY_MONEY,
									dmo.PAY_STATUS,
									dmo.GMT_CREATE,
									'2' TYPE,
									du.USER_NAME,
									du.NICK_NAME,
									du.USER_ID
								FROM
									dxs_media_order dmo
								LEFT JOIN dxs_user du ON dmo.USER_ID = du.USER_ID
						) allorder
					WHERE
						1 = 1
					<isNotEmpty property="nickname">
	    			AND allorder.NICK_NAME like '%$nickname$%'
		    		</isNotEmpty>
		    		<isNotEmpty property="starpdate">
		    			AND allorder.GMT_CREATE &gt;= #starpdate#
		    		</isNotEmpty>
		    		<isNotEmpty property="enddate">
		    			AND allorder.GMT_CREATE &lt;= #enddate#
		    		</isNotEmpty>
		    		<isNotEmpty property="type">
		    			AND allorder.type = #type#
		    		</isNotEmpty>
					ORDER BY
						allorder.GMT_CREATE DESC
				) d
    </select>

