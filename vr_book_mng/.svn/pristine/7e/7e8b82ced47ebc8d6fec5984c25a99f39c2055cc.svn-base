    <!-- mapped statement for IbatisCallOrderDAO.selectallorder -->
	<select id="MS-CALL-ORDER-SELECTALLORDER" resultClass="java.util.HashMap">
	        select allorder.* from 
				(select dco.ORDER_NO,dco.COST,dco.ORDER_STATUS,dco.GMT_CREATE,'0' TYPE,du.USER_NAME,du.NICK_NAME,du.USER_ID,
				 duu.USER_ID RECEIVE_USER_ID,duu.NICK_NAME RECEIVE_NICK_NAME
				from dxs_call_order  dco 
				LEFT JOIN dxs_user du on dco.CALLED_USER_ID = du.USER_ID 
				LEFT JOIN dxs_user duu on dco.CALLING_USER_ID = duu.USER_ID 
				UNION  
				select dmo.ORDER_NO,dmo.PAY_MONEY,dmo.PAY_STATUS,dmo.GMT_CREATE,'1' TYPE,du.USER_NAME,du.NICK_NAME,du.USER_ID,
				dmo.CREATER RECEIVE_USER_ID,dmo.MODIFIER RECEIVE_NICK_NAME
				from dxs_media_order dmo LEFT JOIN  dxs_user du on dmo.USER_ID = du.USER_ID) allorder 
				where 1=1 
				<isNotEmpty property="nickname">
	    			AND allorder.NICK_NAME like '%$nickname$%'
	    		</isNotEmpty>
	    		<isNotEmpty property="starpdate">
	    			AND allorder.GMT_CREATE &gt;= #starpdate#
	    		</isNotEmpty>
	    		<isNotEmpty property="enddate">
	    			AND allorder.GMT_CREATE &lt;= #enddate#
	    		</isNotEmpty>
	    		<isNotEmpty property="type">
	    			AND allorder.type = #type#
	    		</isNotEmpty>
				order  by allorder.GMT_CREATE desc 
				limit $offset$,$limit$
    </select>

