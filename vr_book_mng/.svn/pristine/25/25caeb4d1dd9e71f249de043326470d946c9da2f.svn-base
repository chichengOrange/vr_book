<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="vr_user_login_log">



<operation name="selectLoginType" resultClass="java.util.HashMap" multiplicity="many">
	<sql>
		select * from vr_user_login_log where 1 = 1
	</sql>
	<sqlmap>
		<![CDATA[
		SELECT LOGIN_TYPE  name ,sum(LOGIN_COUNT) value FROM vr_user_login_log  GROUP BY LOGIN_TYPE
		]]>
	</sqlmap>
</operation>
</table>