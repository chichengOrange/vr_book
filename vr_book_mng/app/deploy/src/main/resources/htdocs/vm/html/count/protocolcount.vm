<script type="text/javascript">
	var dom = document.getElementById("protocol-count");
	var myChart = echarts.init(dom);
	var app = {};
	option = null;
	option = {
		title : {
			text : '手机系统统计',
			x : 'center'
		},
		tooltip : {
			trigger : 'item',
			formatter : "{a} <br/>{b} : {c} ({d}%)"
		},
		legend : {
			orient : 'vertical',
			left : 'left',
		},
		series : [ {
			name : '系统',
			type : 'pie',
			radius : '55%',
			center : [ '50%', '60%' ],
			data : [ ],
			itemStyle : {
				emphasis : {
					shadowBlur : 10,
					shadowOffsetX : 0,
					shadowColor : 'rgba(0, 0, 0, 0.5)'
				}
			}
		} ]
	};

	jQuery.ajax({
		url : "selectOs.do",
		type : 'post',
		success : function(data) {
			myChart.setOption({
				series : [ {

					data : data.data

				} ]
			});

		},
		error : function() {
			alert("异常！");
		}
	});
	;
	if (option && typeof option === "object") {
		myChart.setOption(option, true);
	}
</script>
