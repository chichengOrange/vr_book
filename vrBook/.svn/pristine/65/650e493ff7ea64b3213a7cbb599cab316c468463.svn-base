<?xml version="1.0" encoding="GB2312"?>
<!DOCTYPE tables SYSTEM "dalgen-config.dtd">

<!-- ============================================================== -->
<!-- Master configuration file for auto-generation of project dal. -->

<!-- $Id: dal-config.xml,v 1.35 2013/10/29 14:54:07 guangdong.li Exp $ -->

<!-- ============================================================== -->

<tables>
	<!-- ========================================================== -->
	<!-- The typemap("Type Map") maps from one java type to another -->
	<!-- java type. If you feel the original sql data type to java -->
	<!-- type mapping is not satisfactory, you can use typemap to -->
	<!-- convert it to a more appropriate one. -->
	<!-- ========================================================== -->
	<typemap from="java.sql.Date" to="java.util.Date" />
	<typemap from="java.sql.Timestamp" to="java.util.Date" />
	<typemap from="java.math.BigDecimal" to="long" />
	<typemap from="byte" to="int" />
	<typemap from="short" to="int" />

	<tableprefix prefix="vr" />
	
 	<include table="tables/sequence.xml" /> 
	<include table="tables/vr_sys_config.xml" />
	
	<include table="tables/vr_book.xml" />
	<include table="tables/vr_book_user.xml" />
	<include table="tables/vr_sms.xml" />
	<include table="tables/vr_user.xml" />
	<include table="tables/vr_version.xml" />
	<include table="tables/vr_ad.xml" />
	<include table="tables/vr_book_version.xml" />
</tables>


