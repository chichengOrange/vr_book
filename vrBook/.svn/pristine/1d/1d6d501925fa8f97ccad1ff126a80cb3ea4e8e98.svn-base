package com.onway.web.controller.result;

import org.codehaus.jackson.map.annotate.JsonSerialize;


/**
 * 
* <p>Title: ActivityResult</p>  
* <p>Description: 账户信息返回结果集</p>  
* @author yugang.ni  
* @date 2018年6月28日  下午3:26:17
 */
@JsonSerialize(include=JsonSerialize.Inclusion.NON_NULL)
public class MineInfoResult {
	
	private UserInfoResult userAllInfo;

	private AccountResult userAccountInfo;

	private int waitPayCount;
	private int waitSendCount;
	private int waitReceiveCount;
	private int waitCommentCount;
	private int msgCount;
	
	private String userRole;
	
	public UserInfoResult getUserAllInfo() {
		return userAllInfo;
	}
	public void setUserAllInfo(UserInfoResult userAllInfo) {
		this.userAllInfo = userAllInfo;
	}
	public AccountResult getUserAccountInfo() {
		return userAccountInfo;
	}
	public void setUserAccountInfo(AccountResult userAccountInfo) {
		this.userAccountInfo = userAccountInfo;
	}
	public int getWaitPayCount() {
		return waitPayCount;
	}
	public void setWaitPayCount(int waitPayCount) {
		this.waitPayCount = waitPayCount;
	}
	public int getWaitSendCount() {
		return waitSendCount;
	}
	public void setWaitSendCount(int waitSendCount) {
		this.waitSendCount = waitSendCount;
	}
	public int getWaitReceiveCount() {
		return waitReceiveCount;
	}
	public void setWaitReceiveCount(int waitReceiveCount) {
		this.waitReceiveCount = waitReceiveCount;
	}
	public int getWaitCommentCount() {
		return waitCommentCount;
	}
	public void setWaitCommentCount(int waitCommentCount) {
		this.waitCommentCount = waitCommentCount;
	}
	public int getMsgCount() {
		return msgCount;
	}
	public void setMsgCount(int msgCount) {
		this.msgCount = msgCount;
	}
	public String getUserRole() {
		return userRole;
	}
	public void setUserRole(String userRole) {
		this.userRole = userRole;
	}


}
