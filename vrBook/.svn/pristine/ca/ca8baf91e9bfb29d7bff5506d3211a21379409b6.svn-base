package com.onway.fyapp.common.dal.dataobject.returnObj;

import java.util.List;

/**
 * 章 实体类
 * @author lishuaikai
 *
 * 2018年7月20日 下午2:47:20
 */
public class ChapterInfo {

	private String chapterId;
	
	private String chapterName;
	
	private List<SectionsInfo> sectionList;

	public String getChapterId() {
		return chapterId;
	}

	public void setChapterId(String chapterId) {
		this.chapterId = chapterId;
	}

	public String getChapterName() {
		return chapterName;
	}

	public void setChapterName(String chapterName) {
		this.chapterName = chapterName;
	}

	public List<SectionsInfo> getSectionList() {
		return sectionList;
	}

	public void setSectionList(List<SectionsInfo> sectionList) {
		this.sectionList = sectionList;
	}
	
	
	
}
