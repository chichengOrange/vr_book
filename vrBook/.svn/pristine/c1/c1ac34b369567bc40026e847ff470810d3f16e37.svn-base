package com.onway.web.controller.result;

import java.util.List;

import org.codehaus.jackson.map.annotate.JsonSerialize;


/**
 * 
* <p>Title: KnowledgeResult</p>  
* <p>Description: 养生知识/新闻公告表  返回结果集</p>  
* @author yugang.ni  
* @date 2018年6月28日  下午4:13:23
 */
@JsonSerialize(include=JsonSerialize.Inclusion.NON_NULL)
public class KnowledgeResult {
	
	private String knowId;

	private String type;

	private String title;
	
	private String subTitle;
	
	private String content;
	
	private List<String> imgList;
	
	private int praiseCount;
	
	private int commnetCount;
	
	private String time;
	
	private boolean zanOrNot = false;

	public String getKnowId() {
		return knowId;
	}

	public void setKnowId(String knowId) {
		this.knowId = knowId;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getSubTitle() {
		return subTitle;
	}

	public void setSubTitle(String subTitle) {
		this.subTitle = subTitle;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public List<String> getImgList() {
		return imgList;
	}

	public void setImgList(List<String> imgList) {
		this.imgList = imgList;
	}

	public int getPraiseCount() {
		return praiseCount;
	}

	public void setPraiseCount(int praiseCount) {
		this.praiseCount = praiseCount;
	}

	public int getCommnetCount() {
		return commnetCount;
	}

	public void setCommnetCount(int commnetCount) {
		this.commnetCount = commnetCount;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public boolean isZanOrNot() {
		return zanOrNot;
	}

	public void setZanOrNot(boolean zanOrNot) {
		this.zanOrNot = zanOrNot;
	}

	
}
