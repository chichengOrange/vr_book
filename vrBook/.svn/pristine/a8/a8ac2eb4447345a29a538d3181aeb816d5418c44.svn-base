<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="vr_book_chapter">
	
	<!-- 根据书籍id查询 -->
	<operation name="queryChapterByBookId" multiplicity="many">
		<sql>
		    SELECT
				ID,
				BOOK_ID,
				CHAPTER_ID,
				CHAPTER_NAME,
				GMT_CREATE,
				GMT_MODIFIED
			FROM vr_book_chapter
			WHERE BOOK_ID = ?
		</sql>
	</operation>
	
	
</table>