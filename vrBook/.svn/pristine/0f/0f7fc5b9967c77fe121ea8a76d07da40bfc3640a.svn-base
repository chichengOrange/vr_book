package com.onway.web.controller.result;

import org.codehaus.jackson.map.annotate.JsonSerialize;

import com.onway.common.lang.Money;


/**
 * 
* <p>Title: ActivityResult</p>  
* <p>Description: 账户信息返回结果集</p>  
* @author yugang.ni  
* @date 2018年6月28日  下午3:26:17
 */
@JsonSerialize(include=JsonSerialize.Inclusion.NON_NULL)
public class AccountResult {
	
	private Money devoteAmount = new Money();

	private Money huBalance = new Money();

	private Money huPoint = new Money();

	public Money getDevoteAmount() {
		return devoteAmount;
	}

	public void setDevoteAmount(Money devoteAmount) {
		this.devoteAmount = devoteAmount;
	}

	public Money getHuBalance() {
		return huBalance;
	}

	public void setHuBalance(Money huBalance) {
		this.huBalance = huBalance;
	}

	public Money getHuPoint() {
		return huPoint;
	}

	public void setHuPoint(Money huPoint) {
		this.huPoint = huPoint;
	}
	
	
}
