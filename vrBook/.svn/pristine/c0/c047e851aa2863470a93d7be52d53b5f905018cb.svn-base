    <select id="MS-PRODUCT-QUERY-PRODUCT-BY-ITEM" resultMap="RM-PRODUCT">
        SELECT 
					ID,
					PRODUCT_ID,
					TEAM_ID,
					PRODUCT_NAME,
					PRODUCT_PRICE,
					OLD_PRICE,
					STOCK,
					SALES,
					BRAND,
					MANUFACTURER,
					LICENSE_NO,
					TYPE,
					FATHER_CATE,
					CHILD_CATE,
					FATHER_EFECT_CATE,
					CHILD_EFECT_CATE,
					DEL_FLG,
					PRO_TYPE,
					PLAN_ID,
					GMT_CREATE,
					GMT_MODIFIED
				FROM hqyt_product
				WHERE 1=1
				<isNotEmpty property="productId" prepend="and">
					PRODUCT_ID = #productId#
				</isNotEmpty>
				<isNotEmpty property="teamId" prepend="and">
					TEAM_ID = #teamId#
				</isNotEmpty>
				<isNotEmpty property="productName" prepend="and">
					PRODUCT_NAME like '%$productName$%'
				</isNotEmpty>
				<isNotEmpty property="fatherCate" prepend="and">
					FATHER_CATE = #fatherCate#
				</isNotEmpty>
				<isNotEmpty property="childCate" prepend="and">
					CHILD_CATE = #childCate#
				</isNotEmpty>
				<isNotEmpty property="fatherEfectCate" prepend="and">
					FATHER_EFECT_CATE = #fatherEfectCate#
				</isNotEmpty>
				<isNotEmpty property="childEfectCate" prepend="and">
					CHILD_EFECT_CATE = #childEfectCate#
				</isNotEmpty>
				<isNotEmpty property="leastPrice" prepend="and">
					PRODUCT_PRICE <![CDATA[ >= ]]> #leastPrice#
				</isNotEmpty>
				<isNotEmpty property="mostPrice" prepend="and">
					PRODUCT_PRICE <![CDATA[ <= ]]> #mostPrice#
				</isNotEmpty>
				<isNotEmpty property="delFlg" prepend="and">
					DEL_FLG = #delFlg#
				</isNotEmpty>
				<isNotEmpty property="proType" prepend="and">
					PRO_TYPE = #proType#
				</isNotEmpty>
				<isEqual property="salesSort" compareValue="salesDesc">
					order by SALES desc
				</isEqual>
				<isEqual property="salesSort" compareValue="salesAsc">
