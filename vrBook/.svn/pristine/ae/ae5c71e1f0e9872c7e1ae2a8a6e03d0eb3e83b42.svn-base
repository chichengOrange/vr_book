    <!-- mapped statement for IbatisEvaluateDAO.queryEvaluateByProductId -->
    <select id="MS-EVALUATE-QUERY-EVALUATE-BY-PRODUCT-ID" resultClass="java.util.HashMap">
        SELECT
				e.EVALUATE_ID evaluateId,
				e.TYPE type,
				e.USER_ID userId,
				e.PRODUCT_ID productId,
				e.TEAM_ID teamId,
				e.KNOW_ID knowId,
				e.EVALUATE_STAR star,
				e.EVALUATE_CONTENT content,
				e.DEL_FLG delFlg,
				e.GMT_CREATE createTime,
				u.HEAD_URL headUrl,
				u.NICK_NAME nickName,
				i.IMG_URL imgList
			FROM hqyt_evaluate e LEFT JOIN hqyt_user u ON e.USER_ID = u.USER_ID LEFT JOIN hqyt_img i ON e.EVALUATE_ID = i.IMG_ID
            WHERE 1=1
            <isNotEmpty property="productId" prepend="and">
	               e.PRODUCT_ID = #productId#
	        </isNotEmpty>
	        <isNotEmpty property="delFlg" prepend="and">
	               e.DEL_FLG = #delFlg#
	        </isNotEmpty>
	        limit #offset#, #limit#
