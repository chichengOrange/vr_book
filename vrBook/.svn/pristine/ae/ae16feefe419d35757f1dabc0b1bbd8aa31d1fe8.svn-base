package com.onway.web.controller.result;

import org.codehaus.jackson.map.annotate.JsonSerialize;


/**
 * 
* <p>Title: ReduceResult</p>  
* <p>Description: 优惠券  代金券 详情</p>  
* @author yugang.ni  
* @date 2018年6月28日  上午11:21:26
 */
@JsonSerialize(include=JsonSerialize.Inclusion.NON_NULL)
public class ReduceResult {
	
	private int reduceId;
	
	private String reduceType;

	private String reduceTitle;

	private String reduceImg;
	
	private String shopName;
	
	private String canUseMsg;
	
	private String reduceSubTitle;
	
	private String expiryDate;
	
	private String useTime;
	
	private String useArea;
	
	private String useRule;
	
	private String amount;
	
	private String amountOld;

	public int getReduceId() {
		return reduceId;
	}

	public void setReduceId(int reduceId) {
		this.reduceId = reduceId;
	}

	public String getReduceTitle() {
		return reduceTitle;
	}

	public void setReduceTitle(String reduceTitle) {
		this.reduceTitle = reduceTitle;
	}

	public String getReduceImg() {
		return reduceImg;
	}

	public void setReduceImg(String reduceImg) {
		this.reduceImg = reduceImg;
	}

	public String getShopName() {
		return shopName;
	}

	public void setShopName(String shopName) {
		this.shopName = shopName;
	}

	public String getCanUseMsg() {
		return canUseMsg;
	}

	public void setCanUseMsg(String canUseMsg) {
		this.canUseMsg = canUseMsg;
	}

	public String getReduceSubTitle() {
		return reduceSubTitle;
	}

	public void setReduceSubTitle(String reduceSubTitle) {
		this.reduceSubTitle = reduceSubTitle;
	}

	public String getExpiryDate() {
		return expiryDate;
	}

	public void setExpiryDate(String expiryDate) {
		this.expiryDate = expiryDate;
	}

	public String getUseTime() {
		return useTime;
	}

	public void setUseTime(String useTime) {
		this.useTime = useTime;
	}

	public String getUseArea() {
		return useArea;
	}

	public void setUseArea(String useArea) {
		this.useArea = useArea;
	}

	public String getUseRule() {
		return useRule;
	}

	public void setUseRule(String useRule) {
		this.useRule = useRule;
	}

	public String getAmount() {
		return amount;
	}

	public void setAmount(String amount) {
		this.amount = amount;
	}

	public String getAmountOld() {
		return amountOld;
	}

	public void setAmountOld(String amountOld) {
		this.amountOld = amountOld;
	}

	public String getReduceType() {
		return reduceType;
	}

	public void setReduceType(String reduceType) {
		this.reduceType = reduceType;
	}
}
