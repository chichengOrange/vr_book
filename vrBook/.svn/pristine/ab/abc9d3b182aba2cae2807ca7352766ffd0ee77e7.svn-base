package com.onway.model.conver;

import com.onway.fyapp.common.dal.dataobject.BannerDO;
import com.onway.web.controller.result.BannerResult;

/**
 * 
* <p>Title: BannerConver</p>  
* <p>Description: banner实体转换</p>  
* @author yugang.ni  
* @date 2018年6月27日  下午4:34:14
 */
public class BannerConver{
	
	
	public static BannerResult buildBannerResult(BannerDO bannerDO) {
		if(null == bannerDO)
			return null;
		
		BannerResult bannerResult = new BannerResult();
		bannerResult.setBannerId(bannerDO.getId());
		bannerResult.setBannerImg(bannerDO.getBannerImg());
		bannerResult.setBannerType(bannerDO.getBannerType());
		bannerResult.setBannerContent(bannerDO.getBannerContent());
		
		return bannerResult;
    }
	

}
