<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="vr_user_vesion">

	<operation name="insert">
		<sql>
			INSERT INTO vr_user_vesion(
				ID,
				USER_ID,
				APP_VERSION,
				IMEI,
				GMT_CREATE,
				GMT_MODIFIED
			)VALUES(
				?,?,?,?,CURRENT_TIMESTAMP,CURRENT_TIMESTAMP
			)
		</sql>
	</operation>
	
	<operation name="queryByUserId">
		<sql>
			SELECT 
				ID,
				USER_ID,
				APP_VERSION,
				IMEI,
				GMT_CREATE,
				GMT_MODIFIED
			FROM vr_user_vesion WHERE USER_ID = ?
		</sql>
	</operation>
	
	<operation name="updateVersion">
		<sql>
			UPDATE vr_user_vesion SET 
			APP_VERSION = ?,
			GMT_MODIFIED = CURRENT_TIMESTAMP
			WHERE USER_ID = ?
		</sql>
	</operation>
</table>